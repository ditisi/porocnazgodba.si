{
    "version": 2,
    "name": "puslc",
    "regions": ["arn1", "cdg1"],
    "builds": [{
        "src": "package.json",
        "use": "@now/static-build"
    }],
    "routes": [{
        "src": "/service-worker.js",
        "headers": {
            "cache-control": "s-maxage=0"
        }
    }, {
        "handle": "filesystem"
    }, {
        "src": ".*",
        "dest": "/index.html"
    }]
}